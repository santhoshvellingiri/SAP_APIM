        <routeRule>
            <name>default</name>
            <targetEndPointName>default</targetEndPointName>
            <sequence>1</sequence>
            <faultRules/>
        </routeRule>
    </routeRules>
    <faultRules/>
    <preFlow>
        <name>PreFlow</name>
        <request>
            <isRequest>true</isRequest>
            <steps>
                <step>
                    <policy_name>getSharedSecret</policy_name>
                    <condition> </condition>
                    <sequence>1</sequence>
                </step>
                <step>
                    <policy_name>checkSharedSecret</policy_name>
                    <condition> </condition>
                    <sequence>2</sequence>
                </step>
            </steps>
        </request>
        <response>
            <isRequest>true</isRequest>
            <steps/>
        </response>
    </preFlow>
    <postFlow>
        <name>PostFlow</name>
        <request>
            <isRequest>true</isRequest>
            <steps/>
        </request>
        <response>
            <isRequest>true</isRequest>
            <steps/>
        </response>
    </postFlow>
    <conditionalFlows/>
